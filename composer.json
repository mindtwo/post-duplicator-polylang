{
    "name": "mindtwo/post-duplicator-polylang",
    "type": "wordpress-plugin",
    "description": "Removes Polylang 'post_translation' relationships from original post after duplication.",
    "keywords": [
        "wordpress-plugin",
        "post-duplicator",
        "wordpress"
    ],
    "homepage": "https://github.com/mindtwo/post-duplicator-polylang",
    "license": "MIT",
    "authors": [
        {
            "name": "mindtwo GmbH",
            "email": "info@mindtwo.de",
            "homepage": "https://www.mindtwo.de/"
        }
    ],
    "require": {
        "php": "~8.1",
        "wpackagist-plugin/post-duplicator": "^2.20"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://wpackagist.org"
        }
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
